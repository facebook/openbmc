      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 1,
      "negative_hysteresis": 1,
      "data": [
        [63, 15],
        [68, 20],
        [73, 25],
        [78, 30],
        [80, 37],
        [82, 44],
        [84, 50]
      ]
    },
    "pid_gpu_mem": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 88,
      "positive_hysteresis": 1,
      "negative_hysteresis": 2,
      "kp": -3.5,
      "ki": -0.035,
      "kd": 0.0
    },
    "linear_vicor": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [70, 15],
        [75, 20],
        [80, 25],
        [90, 60]
      ]
    }
  },
  "fans": {
    "0": {
      "label" : "Fan 0 Inlet",
      "read_source" : {
        "kv" : "mb_sensor0"
      },
      "write_source" : {
