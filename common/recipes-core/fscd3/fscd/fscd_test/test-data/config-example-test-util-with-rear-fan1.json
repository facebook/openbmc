{
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 3000,
  "boost": {
    "fan_fail": false,
    "sensor_fail": true,
    "progressive": true
  },
  "fan_dead_boost": [
    [1,0],
    [2,100]
  ],
  "fail_sensor_type": {
    "standby_sensor_fail": true,
    "SSD_sensor_fail":false,
    "server_sensor_fail":true
  },
  "watchdog": false,
  "chassis_intrusion": true,
  "min_rpm": 400,
  "profiles": {
    "linear_soc_temp": {
      "read_source" : {
        "util" : "./test-data/get_sensor_data_util.sh sensor-util-test-data2"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 3,
      "data": [
        [ 30, 30 ],
        [ 35, 35 ],
        [ 40, 40 ],
        [ 45, 45 ],
        [ 50, 50 ],
        [ 55, 55 ],
        [ 60, 60 ],
        [ 65, 65 ],
        [ 70, 70 ],
        [ 75, 75 ],
        [ 80, 80 ],
        [ 85, 85 ],
        [ 90, 90 ],
        [ 95, 95 ],
        [ 100, 100 ]
      ]
     },
	  "linear_mezz_sensor_temp": {
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 3,
      "data": [
        [ 48, 30 ],
        [ 52, 34 ],
        [ 56, 38 ],
        [ 60, 42 ],
        [ 64, 46 ],
        [ 68, 50 ],
        [ 72, 54 ],
        [ 76, 58 ],
        [ 80, 62 ],
        [ 84, 66 ],
        [ 88, 70 ],
        [ 92, 80 ],
        [ 96, 90 ],
        [ 100, 100 ]
        ]
    }
  },
  "fans": {
    "0": {
      "label": "Fan 1 Front",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 1"
      },
      "write_source" : {
        "util" : "./test-data/set_sensor_data_util.sh %d 0"
      }
    },
    "1": {
      "label": "Fan 1 Rear",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 2"
      },
      "write_source" : {
        "util" : "./test-data/set_sensor_data_util.sh %d 1"
      }
    },
    "2": {
      "label": "Fan 2 Front",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 3"
      }
    },
    "3": {
      "label": "Fan 2 Rear",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 4"
      }
    },
    "4": {
      "label": "Fan 3 Front",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 5"
      }
    },
    "5": {
      "label": "Fan 3 Rear",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 6"
      }
    },
    "6": {
      "label": "Fan 4 Front",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 7"
      }
    },
    "7": {
      "label": "Fan 4 Rear",
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-with-rear-fans-test-data 8"
      }
    }
  },
  "zones": {
    "zone_0": {
      "pwm_output": [0],
      "expr_file": "zone0-example-with-rear-fans-test1.fsc"
    },
    "zone_1": {
      "pwm_output": [1],
      "expr_file": "zone1-example-with-rear-fans-test1.fsc"
    }
  }
}
