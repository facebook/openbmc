{
    "capabilities": [
        "submit",
        "sandcastle",
        "land",
        "apply"
    ],
    "land_modes": {
        "default": {
            "checkDiffSuccess": true,
            "checks": {
                "allowAfterHours": true,
                "verifyDiffLocalCommitsMatch": true,
                "verifyDiffsAccepted": true,
                "verifyNoMerges": true
            },
            "description": "Normal build gated land",
            "landStyle": "wait_for_all",
            "skipSanityChecks": false
        }
    },
    "postponed_linters": {
        "determinator-lint": [
            {
                "type": "any_regex_matches",
                "value": [
                    "^.*$"
                ]
            }
        ]
    },
    "sandcastle_job_specs": [
        {
            "alias": "openbmc-sandcastle-determinator",
            "args": {
                "name": "openbmc-ci-determinator",
                "oncall": "openbmc",
                "steps": [
                    {
                        "name": "Async lint",
                        "parser": "cat",
                        "report_lint": "determinator-lint",
                        "shell": "arc lint --never-apply-patches --output json"
                    }
                ]
            },
            "capabilities": {
                "tenant": "openbmc",
                "type": "lego",
                "vcs": "openbmc-git"
            },
            "command": "SandcastleUniversalCommand",
            "description": "Yocto diff listener",
            "scheduleType": "diff",
            "tags": [
                "openbmc_git"
            ]
        },
        {
            "alias": "openbmc-cit-default-determinator",
            "args": {
                "name": "openbmc-cit-default-determinator",
                "oncall": "openbmc",
                "steps": [
                    {
                        "determinator": true,
                        "name": "openbmc-cit-default-determinator",
                        "shell": "facebook/sandcastle/job.py cit-diff --platform default"
                    }
                ]
            },
            "capabilities": {
                "tenant": "openbmc",
                "type": "lego",
                "vcs": "openbmc-git"
            },
            "command": "SandcastleUniversalCommand",
            "description": "Yocto diff listener",
            "jellyfish_conditions": [
                {
                    "type": "some_file_matches_some_regexp",
                    "value": [
                        "^openbmc-init-build-env",
                        "^classes/.*",
                        "^common/.*",
                        "^facebook/.*",
                        "^tests2/.*",
                        "^meta-aspeed/.*",
                        "^meta-facebook/recipes-.*/.*",
                        "^meta-facebook/meta-python3/.*",
                        "^meta-facebook/classes/.*",
                        "^meta-facebook/conf/.*",
                        "^meta-facebook/meta-backports/.*",
                        "^yocto/.*"
                    ]
                }
            ],
            "scheduleType": "diff",
            "tags": [
                "openbmc_git"
            ]
        },
