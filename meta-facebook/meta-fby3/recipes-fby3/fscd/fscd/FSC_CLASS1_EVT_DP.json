{
  "version": "fby3-dp-v2-2021.04.07",
  "pwm_transition_value": 60,
  "pwm_boost_value": 100,
  "non_fanfail_limited_boost_value": 100,
  "sample_interval_ms": 3000,
  "sensor_filter_all": true,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "fan_dead_boost": {
    "threshold": 2,
    "action": "host_shutdown"
  },
  "sensor_valid_check": {
    "(.*)front_io_temp": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)soc_therm": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)soc_dimm": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)dp_pcie_temp": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "watchdog": false,
  "min_rpm": 500,
  "profiles": {
    "linear_frontIO_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
