{
  "version": "fbgpv2-brcm-pvt-r01",
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "non_fanfail_limited_boost_value": 90,
  "sample_interval_ms": 3000,
  "sensor_filter_all": true,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "sensor_valid_check": {
    "(.*)soc": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)nvme": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "fail_sensor_type": {
    "SSD_sensor_fail":true,
    "M2_sensor_fail":true,
    "M2_sensor_count":24
  },
  "ssd_progressive_algorithm":{
    "offset_algorithm":[
      [23,20]
    ]
  },
  "watchdog": false,
  "min_rpm": 800,
  "profiles": {
    "linear_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
