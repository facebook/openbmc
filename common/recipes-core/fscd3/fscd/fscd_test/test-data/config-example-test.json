{
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 3000,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "watchdog": false,
  "fanpower": false,
  "min_rpm": 800,
  "profiles": {
    "linear_dimm": {
      "read_source" : {
        "util" : "./test-data/get_sensor_data_util.sh sensor-util-test-data"
      },
      "type": "linear",
      "data": [
        [65, 18],
        [67, 25],
        [70, 30],
        [72, 35],
        [74, 40],
        [76, 50]
      ]
    },
    "linear_cpu_margin": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "data": [
        [ -30, 18 ],
        [ -28, 20 ],
        [ -26, 21 ],
        [ -24, 22 ],
        [ -22, 23 ],
        [ -20, 24 ],
        [ -18, 25 ]
      ]
    },
    "pid_cpu_margin": {
      "read_source" : {
        "util" : "./test-data/get_sensor_data_util.sh sensor-util-test-data"
      },
      "type": "pid",
      "setpoint": -14,
      "negative_hysteresis": 1,
      "positive_hysteresis": 1,
      "kp": -1.25,
      "ki": -0.035,
      "kd": 0
    },
    "linear_mb_outlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "data": [
        [ 43, 18 ],
        [ 45, 19 ],
        [ 47, 21 ],
        [ 49, 23 ]
      ]
    },
    "pid_mb_outlet": {
      "read_source" : {
        "util" : "./test-data/get_sensor_data_util.sh sensor-util-test-data"
      },
      "type": "pid",
      "setpoint": 53,
      "negative_hysteresis": 3,
      "positive_hysteresis": 1,
      "kp": -2,
      "ki": -0.025,
      "kd": 0
    }
  },
  "fans": {
    "0": {
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-test-data 1"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 0"
      }
    },
    "1": {
      "read_source" : {
        "util" : "./test-data/get_fan_data_util.sh ./test-data/test-util-data/fan-util-test-data 2"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 1"
      }
    },
    "2": {
      "read_source" : {
        "util" : "/usr/local/bin/fan-util --get 2"
      },
      "write_source" : {
        "util" : "/usr/local/bin/fan-util --set %d 2"
      }
    }
  },
  "zones": {
    "zone_1": {
      "pwm_output": [0, 1],
      "expr_file": "zone1-example-test.fsc"
    }
  }
}
