{
  "version": "fbep-Nvidia-21y0120",
  "pwm_transition_value": 70,
  "pwm_boost_value": 100,
  "sample_interval_ms": 1000,
  "fan_recovery_time": 1,
  "sensor_filter_all": true,
  "boost": {
    "fan_fail": true,
    "sensor_fail": true
  },
  "sensor_valid_check": {
    "(.*)pax": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)gpu": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)hbm": {
      "attribute": {
        "type" : "power_status"
      }
    },
    "(.*)vicor": {
      "attribute": {
        "type" : "power_status"
      }
    }
  },
  "watchdog": false,
  "fanpower": true,
  "min_rpm": 1500,
  "profiles": {
    "linear_inlet": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 1,
      "negative_hysteresis": 1,
      "data": [
        [23, 15],
        [28, 20],
        [32, 25],
        [37, 35],
        [39, 45],
        [42, 65],
        [45, 80]
      ]
    },
    "linear_switch_remote": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 0,
      "negative_hysteresis": 0,
      "data": [
        [70, 15],
        [75, 20],
        [80, 25],
        [90, 60]
      ]
    },
    "linear_gpu_spot": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "linear",
      "positive_hysteresis": 1,
      "negative_hysteresis": 1,
      "data": [
        [53, 15],
        [58, 20],
        [63, 25],
        [68, 30],
        [70, 37],
        [72, 44],
        [74, 50]
      ]
    },
    "pid_gpu_spot": {
      "read_source" : {
        "util" : "/usr/local/bin/sensor-util"
      },
      "type": "pid",
      "setpoint": 78,
      "positive_hysteresis": 1,
      "negative_hysteresis": 2,
      "kp": -3.5,
      "ki": -0.035,
      "kd": 0.0
